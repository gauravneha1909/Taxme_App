<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:paddingStart="10.0dip"
    android:paddingEnd="10.0dip"
    android:id="@+id/linetwo"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardCornerRadius="@dimen/_10sdp"
        android:layout_margin="@dimen/_5sdp"
       >




    <LinearLayout android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:padding="@dimen/_10sdp"
        android:weightSum="10">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="7">


            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/txtTitle112"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/_10sdp"
                android:layout_weight="6"
                android:ellipsize="end"
                android:justificationMode="inter_word"
                android:text="Employee Name"
                android:textColor="?android:textColorPrimary"
                android:textSize="@dimen/_15sdp" />

            <TextView
                android:id="@+id/txtMessage2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/txtTitle112"
                android:layout_marginTop="@dimen/_10sdp"
                android:text="Start Date"
                android:textSize="@dimen/_12sdp"
                />

            <TextView
                android:id="@+id/txtMessage3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/txtMessage2"
                android:layout_marginTop="@dimen/_5sdp"
                android:text="End Date"
                android:textSize="@dimen/_12sdp"
                />

            <TextView
                android:id="@+id/txtMessage4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/txtMessage3"
                android:layout_marginTop="@dimen/_5sdp"
                android:textSize="@dimen/_12sdp"
                android:text="End Date" />




            <TextView
                android:id="@+id/txtMessage5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/txtMessage4"
                android:layout_marginTop="@dimen/_5sdp"
                android:textSize="@dimen/_12sdp"
                android:text="End Date" />

            <TextView
                android:id="@+id/txtMessage6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/txtMessage5"
                android:layout_marginTop="@dimen/_5sdp"
                android:textSize="@dimen/_12sdp"
                android:text="End Date" />

            <TextView
                android:id="@+id/txtMessage7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/txtMessage6"
                android:layout_marginTop="@dimen/_5sdp"
                android:textSize="@dimen/_12sdp"
                android:text="End Date" />

            <TextView
                android:id="@+id/txtMessage8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/txtMessage7"
                android:layout_marginTop="@dimen/_5sdp"
                android:textSize="@dimen/_12sdp"
                android:text="End Date" />

            <TextView
                android:id="@+id/txtMessage9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/txtMessage8"
                android:layout_marginTop="@dimen/_5sdp"
                android:text="End Date"
                android:textSize="@dimen/_12sdp" />

            <TextView
                android:id="@+id/txtMessage10"
                android:layout_width="match_parent"
                android:ellipsize="end"
                android:maxLines="1"
                android:layout_height="wrap_content"
                android:layout_below="@id/txtMessage9"
                android:layout_marginTop="@dimen/_5sdp"
                android:text=""
                android:textSize="@dimen/_12sdp" />



        </RelativeLayout>

    </LinearLayout>

    </androidx.cardview.widget.CardView>
</LinearLayout>