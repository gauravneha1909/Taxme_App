<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/purple_700"
    >


    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:orientation="vertical"
        android:paddingStart="24.0dip"
        android:paddingEnd="24.0dip">

        <ImageView
            android:layout_width="@dimen/_150sdp"
            android:layout_height="@dimen/_150sdp"
            android:src="@drawable/loadingpage"
            android:layout_gravity="center_horizontal"/>



        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="34.0dip"
            android:orientation="horizontal"
            android:weightSum="10">


            <androidx.appcompat.widget.AppCompatTextView
                style="@style/txtBody"
                android:layout_width="0dp"
                android:layout_height="36.0dip"
                android:layout_weight="5"
                android:alpha="0.7"
                android:gravity="center_vertical"
                android:text="Name" />

            <androidx.appcompat.widget.AppCompatEditText
                android:id="@+id/etFullName"
                style="@style/txtSubTitleTwo"
                android:layout_width="0dp"
                android:layout_height="36.0dip"
                android:layout_gravity="end"
                android:layout_weight="5"
                android:background="@drawable/bg_et_login_black"
                android:hint=""
                android:inputType="textCapWords"
                android:paddingLeft="@dimen/_10sdp"
                android:textColor="@android:color/black"
                android:textColorHint="@color/etHint"
                android:textCursorDrawable="@drawable/color_cursor" />

        </androidx.appcompat.widget.LinearLayoutCompat>


        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="14.0dip"
            android:orientation="horizontal"
            android:weightSum="10">

            <androidx.appcompat.widget.AppCompatTextView
                style="@style/txtBody"
                android:layout_width="0dp"
                android:layout_height="36.0dip"
                android:layout_weight="5"
                android:alpha="0.7"
                android:gravity="center_vertical"
                android:text="Phone Number" />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="36.0dip"
                android:layout_weight="5"
                android:orientation="horizontal"
                android:weightSum="12">

                <RelativeLayout
                    android:id="@+id/linFrequency11111"
                    android:layout_width="0dp"
                    android:layout_height="30.0dip"
                    android:layout_gravity="center"
                    android:layout_weight="5"
                    android:background="@drawable/bg_et_login_black">

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/imgCountyFlag"
                        android:layout_width="@dimen/_15sdp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="8.0dip"
                        android:adjustViewBounds="true" />

                    <androidx.appcompat.widget.AppCompatEditText
                        android:id="@+id/txtCountryCode"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_toRightOf="@id/imgCountyFlag"
                        android:background="@null"
                        android:cursorVisible="false"
                        android:focusable="false"
                        android:gravity="center"
                        android:hint="+91"
                        android:inputType="numberDecimal"
                        android:paddingStart="8.0dip"
                        android:paddingEnd="4.0dip"
                        android:textColor="@android:color/black"
                        android:textColorHint="@android:color/darker_gray"
                        android:textSize="@dimen/_10sdp" />
                </RelativeLayout>


                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="30.0dip"
                    android:layout_gravity="center"
                    android:layout_marginLeft="@dimen/_5sdp"
                    android:layout_weight="7"
                    android:background="@drawable/bg_et_login_black">

                    <androidx.appcompat.widget.AppCompatEditText
                        android:id="@+id/etMobile"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@null"
                        android:gravity="center"
                        android:inputType="numberDecimal"
                        android:paddingStart="8.0dip"
                        android:paddingEnd="4.0dip"
                        android:textColor="@android:color/black"
                        android:textColorHint="@android:color/darker_gray"
                        android:textSize="@dimen/_10sdp" />
                </RelativeLayout>

            </LinearLayout>


        </androidx.appcompat.widget.LinearLayoutCompat>


        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="14.0dip"
            android:orientation="horizontal"
            android:weightSum="10">


            <androidx.appcompat.widget.AppCompatTextView
                style="@style/txtBody"
                android:layout_width="0dp"
                android:layout_height="36.0dip"
                android:layout_weight="5"
                android:alpha="0.7"
                android:gravity="center_vertical"
                android:text="Email Id" />

            <androidx.appcompat.widget.AppCompatEditText
                android:id="@+id/etemailid"
                style="@style/txtSubTitleTwo"
                android:layout_width="0dp"
                android:layout_height="36.0dip"
                android:layout_gravity="end"
                android:layout_weight="5"
                android:background="@drawable/bg_et_login_black"
                android:cursorVisible="false"
                android:ellipsize="end"
                android:focusable="false"
                android:hint=""
                android:inputType="textCapWords"
                android:maxLines="1"
                android:paddingLeft="@dimen/_4sdp"
                android:textColor="@color/etHint"
                android:textColorHint="@color/etHint"
                android:textCursorDrawable="@drawable/color_cursor"
                android:textSize="@dimen/_9sdp" />

        </androidx.appcompat.widget.LinearLayoutCompat>


        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="14.0dip"
            android:orientation="horizontal"
            android:weightSum="10">


            <androidx.appcompat.widget.AppCompatTextView
                style="@style/txtBody"
                android:layout_width="0dp"
                android:layout_height="36.0dip"
                android:layout_weight="5"
                android:alpha="0.7"
                android:gravity="center_vertical"
                android:text="Gender" />

            <androidx.appcompat.widget.AppCompatEditText
                android:id="@+id/slectgender"
                style="@style/txtSubTitleTwo"
                android:layout_width="0dp"
                android:layout_height="36.0dip"
                android:layout_gravity="end"
                android:layout_weight="5"
                android:background="@drawable/bg_et_login_black"
                android:cursorVisible="false"
                android:drawableRight="@drawable/ic_baseline_arrow_drop_down_24"
                android:focusable="false"
                android:focusableInTouchMode="true"
                android:hint=""
                android:inputType="textCapWords"
                android:paddingLeft="@dimen/_10sdp"
                android:textColor="@android:color/black"
                android:textColorHint="@color/etHint"
                android:textCursorDrawable="@drawable/color_cursor" />

        </androidx.appcompat.widget.LinearLayoutCompat>


        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="14.0dip"
            android:orientation="horizontal"
            android:weightSum="10">


            <androidx.appcompat.widget.AppCompatTextView
                style="@style/txtBody"
                android:layout_width="0dp"
                android:layout_height="36.0dip"
                android:layout_weight="5"
                android:alpha="0.7"
                android:gravity="center_vertical"
                android:text="Date Of Birth" />

            <androidx.appcompat.widget.AppCompatEditText
                android:id="@+id/txtDate"
                style="@style/txtSubTitleTwo"
                android:layout_width="0dp"
                android:layout_height="36.0dip"
                android:layout_gravity="end"
                android:layout_weight="5"
                android:background="@drawable/bg_et_login_black"
                android:cursorVisible="false"
                android:focusable="false"
                android:focusableInTouchMode="true"
                android:hint=""
                android:inputType="textCapWords"
                android:paddingLeft="@dimen/_10sdp"
                android:textColor="@android:color/black"
                android:textColorHint="@color/etHint"
                android:textCursorDrawable="@drawable/color_cursor" />

        </androidx.appcompat.widget.LinearLayoutCompat>


        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="32.0dip"
            android:layout_weight="5">

            <com.google.android.material.button.MaterialButton
                android:id="@+id/btnSubmit"
                style="@style/whiteButtonloginhomepage"
                android:layout_width="fill_parent"
                android:layout_height="56.0dip"
                android:layout_marginStart="3.0dip"
                android:layout_marginEnd="3.0dip"
                android:text="Submit"
                android:textAllCaps="false"
                android:textColor="@color/purple_700"
                app:cornerRadius="8.0dip" />
        </FrameLayout>

        <ProgressBar
            android:id="@+id/pbVerifyOtp"
            android:layout_width="36.0dip"
            android:layout_height="36.0dip"
            android:layout_marginStart="16.0dip"
            android:layout_marginTop="32.0dip"
            android:indeterminateTint="@android:color/black"
            android:visibility="gone" />


    </androidx.appcompat.widget.LinearLayoutCompat>

    <ProgressBar android:id="@+id/pbMain" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerHorizontal="true" android:layout_centerVertical="true"
        android:visibility="gone"/>
</RelativeLayout>